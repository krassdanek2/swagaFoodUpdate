            <script>
                //get map element
                const mapBlock = document.querySelector(".locations__map");

                // initialize of map
                function initialize(lat, lng, minDistance) {
                    // default coordinates if haven't custom values
                    if (lat === undefined && lng === undefined) {
                        var center = new google.maps.LatLng(53.3242381, -6.3857848);
                    } else {
                        var center = new google.maps.LatLng(lat, lng);
                    }

                    // default zoom
                    var zoom = 11;
                    // if distance from picked place to nearest location...
                    if (minDistance < 1000) {
                        zoom = 15;
                    } else if (1000 < minDistance < 3000) {
                        zoom = 13;
                    }

                    var map = new google.maps.Map(mapBlock, {
                        zoom: zoom,
                        center: center,
                        mapTypeId: google.maps.MapTypeId.ROADMAP,
                    });

                    // get count of locations
                    var postsCount = 50;

                    var locations = [];

                    // custom marker
                    const image = {
                        url: '/img/icon-map-marker.svg',
                        scaledSize: new google.maps.Size(75, 94),
                    }

                    // get array of locations with lat and lng + info-window
                    for (var i = 0; i < postsCount; i++) {
                        // markers here - array with locations http://screenshots.markupus.tech/9gctf_5jani.png
                        var location = markers[i];

                        locations.push({
                            lat: location.lat,
                            lng: location.lng,
                            info: '<div id="location-infoWin">' + info[i] + '</div>'
                        });
                    }

                    var infoWin = new google.maps.InfoWindow();
                    // Add some markers to the map.
                    // Note: The code uses the JavaScript Array.prototype.map() method to
                    // create an array of markers based on a given "locations" array.
                    // The map() method here has nothing to do with the Google Maps API.
                    var newMarkers = locations.map(function(location, i) {
                        var marker = new google.maps.Marker({
                            position: location,
                            icon: image
                        });

                        // show infoWindow on marker click and push location info in it
                        google.maps.event.addListener(marker, 'click', function(evt) {
                            infoWin.setContent(location.info);
                            infoWin.open(map, marker);
                        })
                        return marker;
                    });

                    // custom markers for clustering
                    var options = {
                        styles: [{
                            'url': 'https://www.whopper.ie/wp-content/themes/burger_king/static/img/marker1.png',
                            'height': '117',
                            'width': '93',
                            'textColor': '#D62300'
                        },
                            {
                                'url': 'https://www.whopper.ie/wp-content/themes/burger_king/static/img/marker2.png',
                                'height': '117',
                                'width': '93',
                                'textColor': '#D62300'
                            },
                            {
                                'url': 'https://www.whopper.ie/wp-content/themes/burger_king/static/img/marker3.png',
                                'height': '117',
                                'width': '93',
                                'textColor': '#D62300'
                            }
                        ]
                    };

                    // clustering init
                    var markerCluster = new MarkerClusterer(map, newMarkers, options);
                }

                google.maps.event.addDomListener(window, "load", initialize());
            </script>
