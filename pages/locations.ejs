                                let locations = document.querySelectorAll(".location");
                                locations = Array.from(locations);
                                locations.sort(({
                                                    dataset: {
                                                        distance: a
                                                    }
                                                }, {
                                                    dataset: {
                                                        distance: b
                                                    }
                                                }) => a - b)
                                    .forEach((item) => item.parentNode.appendChild(item));

                                const minDistance = locations[0].dataset.distance

                                initialize(clientLat, clientLng, minDistance);
                            }

                            function getCurrentCoordinates() {
                                var options = {
                                    enableHighAccuracy: true,
                                    maximumAge: 0
                                };

                                function success(pos) {
                                    var clientCoords = pos.coords;
                                    calculateDistance(clientCoords.latitude, clientCoords.longitude);
                                };

                                function error(err) {
                                    console.warn(`ERROR(${err.code}): ${err.message}`);
                                };

                                navigator.geolocation.getCurrentPosition(success, error, options);
                            }

                            getCurrentCoordinates();
                        </script>

                    </div>


                </div>
            </div>

