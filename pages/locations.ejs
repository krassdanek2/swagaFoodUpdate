                        <script>
                            function calculateDistance(clientLat, clientLng) {

                                var locations = document.querySelectorAll('.location'),
                                    distance = [],
                                    distanceVal = [];

                                for (var i = 0; i < locations.length; i++) {
                                    var location = locations[i],
                                        locationLat = location.dataset.lat,
                                        locationLng = location.dataset.lng;

                                    let distances = getDistanceFromLatLonInKm(clientLat, clientLng, locationLat, locationLng);

                                    distance.push(distances + ' km');
                                    distanceVal.push(distances);

                                    // if all locations in array - paste them to page
                                    if (distance.length === locations.length && distanceVal.length === locations.length) {
                                        pasteDistance(distance, locations, distanceVal, clientLat, clientLng);
                                    }
                                }
                            }

                            // distance formula
                            function getDistanceFromLatLonInKm(lat1, lon1, lat2, lon2) {
                                var R = 6371; // Radius of the earth in km
                                var dLat = deg2rad(lat2 - lat1); // deg2rad below
                                var dLon = deg2rad(lon2 - lon1);
                                var a =
                                    Math.sin(dLat / 2) * Math.sin(dLat / 2) +
                                    Math.cos(deg2rad(lat1)) * Math.cos(deg2rad(lat2)) *
                                    Math.sin(dLon / 2) * Math.sin(dLon / 2);
                                var c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));
                                var d = R * c; // Distance in km
                                d = d.toFixed(2);
                                d = Number(d);
                                return d;
                            }

                            function deg2rad(deg) {
                                return deg * (Math.PI / 180)
                            }

                            function pasteDistance(distance, locations, distanceVal, clientLat, clientLng) {
                                for (var i = 0; i < locations.length; i++) {
                                    var location = locations[i],
                                        distanceField = location.querySelector('.location__distance');

                                    location.dataset.distance = distanceVal[i];
                                    distanceField.innerHTML = ' • ' + distance[i];
                                }
                                sortLocations(clientLat, clientLng);
                            }

