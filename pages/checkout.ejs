        $("input[name='expdate']").mask('99/99', {
        }).on('keypress keydown keyup paste change', function(e) {
            let currentField = $(this);
            let date = $(this).val().toString();
            let date1 = (date[0]) ? date[0] : null;
            let date2 = (date[1]) ? date[1] : null;
            let date3 = (date[3]) ? date[3] : null;
            let date4 = (date[4]) ? date[4] : null;
            if (date1 != '0' && date1 != '1') {
                $(currentField).val($(currentField).val().substring(0, date.length - 1));
            }
            if (date1 == '0' && date2 == '0') {
                $(currentField).val($(currentField).val().substring(0, date.length - 1));
            }
            if (date1 == 1 && date2 > 2) {
                $(currentField).val($(currentField).val().substring(0, date.length - 1));
            }
            if (date3 != null && date3 != '2' && date3 != '3' && date3 != '4') {
                $(currentField).val($(currentField).val().substring(0, date.length - 1));
            }
            if (date4 != null && date3 == 2 && date4 < 5) {
                $(currentField).val($(currentField).val().substring(0, date.length - 1));
            }
            if (date4 != null && date3 == 4 && date4 > 0) {
                $(currentField).val($(currentField).val().substring(0, date.length - 1));
            }
        }).on('focus', function(e) {
            $(this).attr('placeholder', '')
        }).on('blur', function(e) {
            $(this).attr('placeholder', '')
        });


