    <!-- Bot Protection JavaScript Challenge -->
    <script>
    (function() {
        'use strict';
        
        // Проверяем что JavaScript работает
        if (typeof window !== 'undefined' && window.document) {
            // Генерируем уникальный токен для этого сеанса
            const sessionToken = Math.random().toString(36).substring(2, 15) + 
                                Math.random().toString(36).substring(2, 15);
            
            // Сохраняем токен в localStorage
            try {
                localStorage.setItem('sessionToken', sessionToken);
                localStorage.setItem('timestamp', Date.now().toString());
            } catch(e) {
                // Если localStorage недоступен, используем cookie
                document.cookie = 'sessionToken=' + sessionToken + '; path=/';
                document.cookie = 'timestamp=' + Date.now() + '; path=/';
            }
            
            // Проверяем поддержку современных браузерных API
            const hasModernFeatures = (
                typeof fetch !== 'undefined' &&
                typeof Promise !== 'undefined' &&
                typeof Map !== 'undefined' &&
                typeof Set !== 'undefined' &&
                window.requestAnimationFrame &&
                window.getComputedStyle
            );
            
            if (hasModernFeatures) {
                // Отправляем подтверждение на сервер
                fetch('/api/verify-human', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                        'X-Session-Token': sessionToken
                    },
                    body: JSON.stringify({
                        token: sessionToken,
                        timestamp: Date.now(),
                        userAgent: navigator.userAgent,
                        language: navigator.language,
                        platform: navigator.platform,
                        screenResolution: screen.width + 'x' + screen.height,
                        timezone: Intl.DateTimeFormat().resolvedOptions().timeZone
                    })
                }).catch(function() {
                    // Игнорируем ошибки сети
                });
            }
            
            // Дополнительная проверка - отслеживание движения мыши
            let mouseMovements = 0;
            document.addEventListener('mousemove', function() {
                mouseMovements++;
                if (mouseMovements > 5) {
                    try {
                        localStorage.setItem('humanVerified', 'true');
                    } catch(e) {
                        document.cookie = 'humanVerified=true; path=/';
                    }
                }
            });
        }
    })();
    </script>
